
<div class="wrapLeft">
  <% unless !@result.is_review_for?(@current_user, @assignment) %>
    <%= render partial: 'results/student/completed_peer_reviews',
               locals: { current_user: @current_user,
                         assignment: @assignment,
                         result: @result,
                         submission: @submission,
                         prs: @prs} %>
  <% end %>

  <p class="notice"><%= I18n.t(:no_result)%></p>
</div>
