<div id="peer_reviews">
  <p id='peer_review_selector_label'>
    <%= @current_user.student? ? "Reviews made on your submission:" : "This reviewer's reviews: " %>
  </p>

  <div id='peer_review_selector' class='dropdown'>
    <a><%= "Review " + current_pr.id.to_s %></a>
    <ul>
      <% prs.each do |pr| %>
        <li>
          <%= link_to("Review " + pr.id.to_s,
                      view_marks_assignment_result_path(assignment.id, pr.result_id)) %>
        </li>
      <% end %>
    </ul>
  </div>

  <%= link_to("View TA Result",
              view_marks_assignment_submission_result_path(assignment.id, submission.id,
                                                              submission.get_latest_result.id),
               id: 'original_result_link') %>
</div>