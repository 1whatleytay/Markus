<% content_for :head do %>
  <%= javascript_tag nonce: true do %>
    document.addEventListener('DOMContentLoaded', () => {
      makeAdminTable(document.getElementById('admins_table'), {course_id: <%= @current_course.id %>});
    })
  <% end %>
<% end %>

<% content_for :title do %>
  <div class='title-help'>
    <p>
      <%= t('admins.help.manage_admins') %>
    </p>
  </div>
<% end %>

<% @heading_buttons = [
  { link_text: t('admins.new'),
    link_path: new_course_admin_path(@current_course) }
] %>

<div id='admins_table'></div>
