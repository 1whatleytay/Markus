<%= content_for :head do %>
  <%= javascript_include_tag 'marking_schemes' %>
  <%= render partial: 'marking_schemes_table', formats: [:'js.jsx'], handlers: [:erb] %>
<% end %>

<div class='title_bar'>
  <h1>
    Marking Schemes
  </h1>
  <div class='heading_buttons'>
    <a class="new_ms_link" href="#">Add New Marking Scheme</a>
  </div>
</div>

<div class='wrapLeft'>
    <div id='marking_schemes_table'></div>
</div>

<div class="marking_scheme_form">
  <h2>Add a new Marking Scheme</h2>
  <%= render partial: 'form',
             formats: [:html],
             handlers: [:erb] %>
</div>           